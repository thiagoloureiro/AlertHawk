	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="AlertHawk.Notification.Domain" Version="1.0.8" />
		<PackageReference Include="ClickHouse.Client" Version="7.14.0" />
		<PackageReference Include="Azure.Identity" Version="1.17.1" />
